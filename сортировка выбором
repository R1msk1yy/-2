#include <iostream>
using namespace std;

int main() {
    const int N = 10; // Определяем константу N, равную 10
    int a[N] = { 1, 25, 6, 32, 43, 5, 96, 23, 4, 55 }; // Инициализируем массив a размером N с заданными значениями

    int min = 0; // для записи минимального значения
    int buf = 0; // временная переменная для обмена значениями

/********************* начало сортировки***********/
    for (int i = 0; i < N; i++) {
        min = i; // запомним номер текущей ячейки, как ячейки с минимальным значением
// в цикле найдем реальный номер ячейки с минимальным значением

        for (int j = i + 1; j < N; j++) {
            min = (a[j] < a[min]) ? j : min;
// сделаем перестановку этого элемента, поменяв его местами с текущим
        }

        if (i != min) { // Обмен значениями a[i] и a[min] 
            buf = a[i]; // Сохраняем значение a[i] во временной переменной
            a[i] = a[min]; // Присваиваем a[minIndex] к a[i]
            a[min] = buf; // Помещаем сохраненное значение в a[minIndex]
        }
    }
/********************* конец сортировки***********/
    for (int i = 0; i < N; i++) // вывод отсортированого массива
        cout << a[i] << "\t"; // Печатаем каждый элемент массива
    cout << endl; // Печатаем перевод строки после вывода

    return 0;
}
